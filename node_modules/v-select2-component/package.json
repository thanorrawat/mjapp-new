{
  "name": "v-select2-component",
  "version": "0.4.3",
  "description": "select2 component in vue.",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/godbasin/vue-select2.git"
  },
  "keywords": [
    "vue",
    "select2"
  ],
  "author": "godbasin <wangbeishan@163.com> (https://github.com/godbasin)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/godbasin/vue-select2/issues"
  },
  "main": "dist/Select2.ssr.js",
  "module": "dist/Select2.esm.js",
  "unpkg": "dist/Select2.min.js",
  "browser": {
    "./sfc": "src/Select2.vue"
  },
  "files": [
    "dist/*",
    "src/**/*.vue"
  ],
  "scripts": {
    "build": "cross-env NODE_ENV=production rollup --config build/rollup.config.js",
    "build:ssr": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format cjs",
    "build:es": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format es",
    "build:unpkg": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format iife"
  },
  "homepage": "https://github.com/godbasin/vue-select2#readme",
  "dependencies": {
    "jquery": "^3.3.1",
    "select2": "^4.0.7-rc.0"
  },
  "devDependencies": {
    "@babel/core": "^7.4.4",
    "@babel/preset-env": "^7.4.4",
    "babel-plugin-syntax-jsx": "^6.18.0",
    "babel-plugin-transform-vue-jsx": "^3.7.0",
    "cross-env": "^5.2.0",
    "minimist": "^1.2.0",
    "rollup": "^1.12.1",
    "rollup-plugin-babel": "^4.3.2",
    "rollup-plugin-commonjs": "^10.0.0",
    "rollup-plugin-replace": "^2.2.0",
    "rollup-plugin-terser": "^4.0.4",
    "rollup-plugin-vue": "^5.0.0",
    "vue": "^2.6.10",
    "vue-template-compiler": "^2.6.10"
  }
}
